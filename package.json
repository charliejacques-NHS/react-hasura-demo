{
  "name": "react-hasura-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "type:check": "tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier .",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "validate": "run-p lint format:check type:check",
    "preview": "vite preview",
    "prepare": "husky install",
    "start:server": "node server.js",
    "dev:server": "nodemon server.js",
    "build:server": "npm run clean:server && npm run transpile:server",
    "clean:server": "rm -rf server-dist",
    "transpile:server": "babel server.js -d server-dist --copy-files --ignore node_modules",
    "prestart:server": "npm run build:server",
    "create-json-translations": "node src/assets/translations/CSV2JSON.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.19.2",
    "i18next": "^23.10.1",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-activity": "^2.1.3",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.22.3",
    "sass": "^1.72.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.4",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@babel/cli": "^7.18.0",
    "@babel/core": "^7.18.0",
    "@babel/node": "^7.18.1",
    "@babel/preset-env": "^7.18.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.6"
  }
}